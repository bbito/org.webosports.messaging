{
	"id": "org.webosports.messaging.service",
	"description": "webOS Ports messaging service. Assigns messages to chat threads.",
	"engine": "node",
	"activityTimeout": 30,
	"commandTimeout": 60,
	"services": [
		{
			"name": "org.webosports.messaging.service",
			"description": "webOS Ports messaging service. Assigns messages to chat threads.",
			"globalized": false,
			"privatebus": true,
			"commands": [
				{
					"name": "assignMessages",
					"assistant": "AssignMessages",
					"public": false
				},
				{
					"name": "putMessage",
					"assistant": "PutMessage",
					"public": false
				},
				{
					"name": "deleteChatthread",
					"assistant": "DeleteChatthread",
					"public": false
				},
				{
					"name": "updateThreadValues",
					"assistant": "UpdateThreadValues",
					"public": false
				}
			]
		}
	]
}
